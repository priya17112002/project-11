#include<stdio.h>  
#include <string.h>    
struct employee{    
int empno;    
char empname[20];
char age[20];
int salary;   
};
int main()
{    
int i;    
struct employee st[20];    
printf("Enter Records of 20 employees");    
for(i=0;i<20;i++)
{    
printf("\nEnter empno:");    
scanf("%d",&st[i].empno);    
printf("\nEnter empName:");    
scanf("%s",&st[i].empname); 
printf("\nEnter age:");    
scanf("%s",&st[i].age);    
printf("\nEnter salary:");    
scanf("%d",&st[i].salary); 
}    
printf("\nemployee Information List:");    
for(i=0;i<20;i++)
{    
printf("\nempno:%d, empname:%s, age:%s, salary:%d ",st[i].empno,st[i].empname,st[i].age,st[i].salary);    
}    
   return 0;    
}
